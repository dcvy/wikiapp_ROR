
<table>
<% @wikis.each do |wiki| %>
  <tr>
    <td><%= link_to wiki.title, wiki_path(wiki) %></td>
    <td><%= button_to 'Edit', edit_wiki_path(wiki), method: 'get'%></td>
    <td><%= button_to 'Remove', wiki_path(wiki), method: 'delete', data: { confirm: 'Are you sure?' } %></td>
<% end %>
</table>
<%= link_to "Add a New Wiki", new_wiki_path %>